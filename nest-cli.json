{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/gateway/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/gateway/tsconfig.app.json"
  },
  "projects": {
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "rabbit": {
      "type": "library",
      "root": "libs/rabbit",
      "entryFile": "index",
      "sourceRoot": "libs/rabbit/src",
      "compilerOptions": {
        "tsConfigPath": "libs/rabbit/tsconfig.lib.json"
      }
    },
    "gateway": {
      "type": "application",
      "root": "apps/gateway",
      "entryFile": "main",
      "sourceRoot": "apps/gateway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/gateway/tsconfig.app.json"
      }
    },
    "user": {
      "type": "application",
      "root": "apps/user",
      "entryFile": "main",
      "sourceRoot": "apps/user/src",
      "compilerOptions": {
        "tsConfigPath": "apps/user/tsconfig.app.json"
      }
    },
    "auth": {
      "type": "application",
      "root": "apps/auth",
      "entryFile": "main",
      "sourceRoot": "apps/auth/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth/tsconfig.app.json"
      }
    },
    "vehicle": {
      "type": "application",
      "root": "apps/vehicle",
      "entryFile": "main",
      "sourceRoot": "apps/vehicle/src",
      "compilerOptions": {
        "tsConfigPath": "apps/vehicle/tsconfig.app.json"
      }
    },
    "database": {
      "type": "library",
      "root": "libs/database",
      "entryFile": "index",
      "sourceRoot": "libs/database/src",
      "compilerOptions": {
        "tsConfigPath": "libs/database/tsconfig.lib.json"
      }
    },
    "language": {
      "type": "library",
      "root": "libs/language",
      "entryFile": "index",
      "sourceRoot": "libs/language/src",
      "compilerOptions": {
        "tsConfigPath": "libs/language/tsconfig.lib.json"
      }
    },
    "token": {
      "type": "library",
      "root": "libs/token",
      "entryFile": "index",
      "sourceRoot": "libs/token/src",
      "compilerOptions": {
        "tsConfigPath": "libs/token/tsconfig.lib.json"
      }
    },
    "otp": {
      "type": "application",
      "root": "apps/otp",
      "entryFile": "main",
      "sourceRoot": "apps/otp/src",
      "compilerOptions": {
        "tsConfigPath": "apps/otp/tsconfig.app.json"
      }
    },
    "authentication": {
      "type": "library",
      "root": "libs/authentication",
      "entryFile": "index",
      "sourceRoot": "libs/authentication/src",
      "compilerOptions": {
        "tsConfigPath": "libs/authentication/tsconfig.lib.json"
      }
    },
    "policy": {
      "type": "library",
      "root": "libs/policy",
      "entryFile": "index",
      "sourceRoot": "libs/policy/src",
      "compilerOptions": {
        "tsConfigPath": "libs/policy/tsconfig.lib.json"
      }
    },
    "storage": {
      "type": "application",
      "root": "apps/storage",
      "entryFile": "main",
      "sourceRoot": "apps/storage/src",
      "compilerOptions": {
        "tsConfigPath": "apps/storage/tsconfig.app.json"
      }
    }
  },
  "monorepo": true,
  "root": "apps/gateway"
}